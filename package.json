{
  "name": "waterbrooke",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf ./build && rm -rf ./dist",
    "build": "next build ./src && next export -o dist ./src",
    "dev": "next ./src",
    "deploy:beta": "aws s3 sync ./dist s3://beta.waterbrooke.net --delete",
    "deploy:prod": "aws s3 sync ./dist s3://waterbrooke.net --delete",
    "deploy:beta:soon": "aws s3 sync ./dist s3://beta.waterbrooke.net --delete && aws cloudfront create-invalidation --distribution-id E1TFZ3ZOYUB1SW --paths \"/*\"",
    "deploy:prod:soon": "aws s3 sync ./dist s3://waterbrooke.net --delete && aws cloudfront create-invalidation --distribution-id E1TFZ3ZOYUB1SW --paths \"/*\"",
    "lint": "eslint \"./**/*.{js,jsx}\"",
    "gen:comp": "node ./node_modules/@ebsolutions/generate-react-templates --type=COMP",
    "gen:cont": "node ./node_modules/@ebsolutions/generate-react-templates --type=CONT",
    "gen:page": "node ./node_modules/@ebsolutions/generate-react-templates --type=PAGE"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronsisler/clients.waterbrooke.git"
  },
  "author": "Aaron Sisler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aaronsisler/clients.waterbrooke/issues"
  },
  "homepage": "https://github.com/aaronsisler/clients.waterbrooke#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@hookform/resolvers": "2.8.3",
    "axios": "^0.21.4",
    "classnames": "^2.2.6",
    "email-validator": "^2.0.3",
    "next": "^12.0.10",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.20.2",
    "sass": "^1.43.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@ebsolutions/generate-react-templates": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.4",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "stylelint": "^14.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0"
  }
}
